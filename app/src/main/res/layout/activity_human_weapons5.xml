<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="amardhebardevelopments.halo5soundboard.HumanWeaponsActivity5">

    <ImageButton
        android:id="@+id/spartanlaser"
        android:layout_width="385dp"
        android:layout_height="161dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="6dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/spartanlaser" />

    <ImageButton
        android:id="@+id/chaingunturret"
        android:layout_width="387dp"
        android:layout_height="172dp"
        app:srcCompat="@mipmap/chaingunturret"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.474" />

    <ImageButton
        android:id="@+id/rocketpodlauncher"
        android:layout_width="392dp"
        android:layout_height="180dp"
        app:srcCompat="@mipmap/rocketpodlauncher"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        tools:layout_editor_absoluteY="331dp"
        tools:layout_editor_absoluteX="0dp" />
</android.support.constraint.ConstraintLayout>
