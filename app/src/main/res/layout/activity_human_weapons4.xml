<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="amardhebardevelopments.halo5soundboard.HumanWeaponsActivity4">

    <ImageButton
        android:id="@+id/saw"
        android:layout_width="375dp"
        android:layout_height="150dp"
        android:layout_marginTop="26dp"
        app:srcCompat="@mipmap/saw"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        />
    <ImageButton
        android:id="@+id/rocketlauncher"
        android:layout_width="375dp"
        android:layout_height="150dp"
        app:srcCompat="@mipmap/rocketlauncher"
        tools:layout_editor_absoluteY="157dp"
        android:layout_below="@+id/saw"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/sniperrifle"
        android:layout_width="375dp"
        android:layout_height="150dp"
        app:srcCompat="@mipmap/sniperrifle"
        android:layout_below="@+id/rocketlauncher"
        tools:layout_editor_absoluteY="324dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/page4right"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="183dp"
        android:layout_marginStart="183dp"
        android:text=">"
        android:layout_alignLeft="@+id/sniperrifle"
        android:layout_alignStart="@+id/sniperrifle"
        android:layout_alignTop="@+id/page4left" />

    <Button
        android:id="@+id/page4left"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignLeft="@+id/sniperrifle"
        android:layout_alignStart="@+id/sniperrifle"
        android:layout_below="@+id/rocketlauncher"
        android:layout_marginRight="183dp"
        android:layout_marginTop="152dp"
        android:text="<" />

</RelativeLayout>
